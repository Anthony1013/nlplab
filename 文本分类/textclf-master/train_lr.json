{
    "__class__": "MLTrainerConfig",
    "params": {
        "raw_data_path": "textclf.joblib",
        "save_dir": "ckpts/",
        "vectorizer": {
            "__class__": "CountVectorizerConfig",
            "params": {
                "lowercase": true,
                "stop_words": null,
                "token_pattern": "(?u)\\b\\w+\\b",
                "ngram_range": [
                    1,
                    1
                ],
                "max_df": 1.0,
                "min_df": 1,
                "max_features": null,
                "binary": false
            }
        },
        "model": {
            "__class__": "LogisticRegressionConfig",
            "params": {
                "penalty": "l2",
                "dual": false,
                "tol": 0.0001,
                "C": 1.0,
                "fit_intercept": true,
                "intercept_scaling": 1,
                "class_weight": "balanced",
                "solver": "sag",
                "max_iter": 1000,
                "multi_class": "ovr",
                "verbose": 0,
                "random_state": null,
                "n_jobs": null,
                "l1_ratio": null
            }
        }
    }
}